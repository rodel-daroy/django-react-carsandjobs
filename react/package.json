  "dependencies": {
    "airbnb-prop-types": "^2.13.2",
    "animate.css": "^3.7.2",
    "big.js": "^5.2.2",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.3",
    "country-json": "^1.0.9",
    "css-element-queries": "^1.2.0",
    "draft-js": "^0.10.5",
    "file-saver": "^2.0.2",
    "flickity": "^2.2.0",
    "gsap": "^2.1.3",
    "json2csv": "^4.5.1",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.4",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "path-to-regexp": "^3.0.0",
    "paypal-checkout": "^4.0.271",
    "postscribe": "^2.0.8",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-datepicker": "^1.8.0",
    "react-dnd": "^9.3.4",
    "react-dnd-html5-backend": "^9.3.4",
    "react-dom": "^16.8.6",
    "react-dropzone": "^4.3.0",
    "react-loadable": "^5.5.0",
    "react-mde": "^5.8.0",
    "react-media": "^1.9.2",
    "react-meta-tags": "^0.4.2",
    "react-modal": "^3.8.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-scrollspy": "^3.4.0",
    "react-select": "^1.3.0",
    "react-simplemde-editor": "^4.0.1",
    "react-sticky-el": "^1.0.20",
    "react-transition-group": "^1.2.1",
    "react-waypoint": "^8.1.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2",
    "remarkable": "^1.7.1",
    "reselect": "^4.0.0",
    "turndown": "^5.0.3",
    "url-search-params-polyfill": "^4.0.1",
    "use-debounce": "^2.2.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build-modernizr": "node ./config/modernizr.js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "npm run build-modernizr && react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "npm run build-modernizr && react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-aws-s3 --bucket-path=carsandjobs-storybook --aws-profile=dev_admin",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-email:test": "node ./config/buildEmailTemplates.js https://test.carsandjobs.com",
    "build-email:prod": "node ./config/buildEmailTemplates.js https://www.carsandjobs.com",
    "clean:test": "aws s3 rm s3://test-carsandjobs-com --recursive --profile dev_admin",
    "sync:test": "aws s3 sync build/ s3://test-carsandjobs-com --profile dev_admin",
    "invalidate:test": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1M8KMI619CSZU --paths '/*'",
    "clean:prod": "aws s3 rm s3://cars-n-jobs --recursive --profile prod_admin",
    "sync:prod": "aws s3 sync build/ s3://cars-n-jobs --profile prod_admin",
    "invalidate:prod": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1837NFEHFIRHR --paths '/*' && aws cloudfront create-invalidation --distribution-id E18RTG9218OJTQ --paths '/*'",
    "deploy:test": "npm ci && npm run build-email:test && REACT_APP_ENV=test npm run build && npm run clean:test && npm run sync:test && npm run invalidate:test",
    "deploy:prod": "npm ci && npm run build-email:prod && REACT_APP_ENV=production GENERATE_SOURCEMAP=false npm run build && npm run clean:prod && npm run sync:prod && npm run invalidate:prod",
    "deploy:test:fast": "npm run build-email:test && REACT_APP_ENV=test npm run build && npm run clean:test && npm run sync:test && npm run invalidate:test"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "acorn-dynamic-import": "^3.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-runtime": "6.26.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^12.2.0",
    "mkdirp": "^0.5.1",
    "modernizr": "^3.7.1",
    "redux-devtools-extension": "^2.13.8",
    "source-map-explorer": "^1.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
